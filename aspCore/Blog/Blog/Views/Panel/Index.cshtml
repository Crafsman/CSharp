@model IEnumerable<Post>

<h2>Admin page!</h2>

<div>
    <ul>
        @foreach (var post in Model)
        {

            <li>
                @post.Title
                -
                <a asp-controller="Panel" asp-action="Edit" asp-route-id="@post.ID">
                    Edit
                </a>
                -
                <a asp-controller="Panel" asp-action="Remove" asp-route-id="@post.ID">
                    Remove
                </a>
            </li>
        }
    </ul>
</div>